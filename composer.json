{
    "name": "esign/laravel-linkable",
    "description": "Dynamically link Laravel models",
    "keywords": [
        "esign",
        "linkable"
    ],
    "homepage": "https://github.com/esign/laravel-linkable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Bart Decorte",
            "email": "bart@esign.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/view": "^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\Linkable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\Linkable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\Linkable\\LinkableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
